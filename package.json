{
	"private": true,
	"name": "next-boilerplate",
	"version": "0.1.0",
	"scripts": {
		"build": "cross-env TZ=UTC next build && cross-env TZ=UTC tsc --project tsconfig.server.json",
		"ci": "npm run lint && npm run test && npm run build",
		"dev": "nodemon",
		"lint": "eslint 'src/**/*.{js,ts,tsx,json}' '*.{js,ts,tsx,json}' '.storybook/**/*.{js,ts,tsx,json}'",
		"lint:fix": "npm run lint -- --fix",
		"ts": "tsc --project . && tsc --project tsconfig.server.json",
		"start": "cross-env TZ=UTC NODE_ENV=production node dist/index.js",
		"storybook": "start-storybook --ci -p 6006 -c .storybook",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"generate:schema": "ts-node src/tasks/generate-schema.ts"
	},
	"dependencies": {
		"@sentry/browser": "^5.24.2",
		"@sentry/integrations": "^5.24.2",
		"@sentry/node": "^5.24.2",
		"@vercel/node": "^1.8.3",
		"@zeit/next-source-maps": "^0.0.4-canary.1",
		"eslint-plugin-react": "7.21.2",
		"eslint-plugin-react-hooks": "4.1.2",
		"graphql-zeus": "^2.8.3",
		"next": "9.5.4",
		"nodemon": "^2.0.4",
		"react": "16.13.1",
		"react-dom": "16.13.1"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/preset-env": "^7.11.5",
		"@babel/preset-flow": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/preset-typescript": "^7.10.4",
		"@storybook/addon-controls": "^6.0.22",
		"@storybook/addon-docs": "^6.0.22",
		"@storybook/preset-typescript": "^3.0.0",
		"@storybook/react": "^6.0.22",
		"@types/enzyme": "^3.10.7",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^26.0.14",
		"@types/node": "14.11.2",
		"@types/react": "16.9.49",
		"@types/react-test-renderer": "^16.9.3",
		"@typescript-eslint/eslint-plugin": "4.3.0",
		"@typescript-eslint/parser": "4.3.0",
		"babel-jest": "^26.3.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"babel-preset-react-app": "^9.1.2",
		"cross-env": "^7.0.2",
		"css-loader": "^4.3.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "7.10.0",
		"eslint-config-prettier": "6.12.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest": "^26.4.2",
		"prettier": "2.1.2",
		"react-test-renderer": "^16.13.1",
		"sass": "^1.26.11",
		"sass-loader": "^10.0.2",
		"style-loader": "^1.2.1",
		"ts-jest": "^26.4.1",
		"ts-node": "9.0.0",
		"typescript": "4.0.3"
	}
}
