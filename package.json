{
	"private": true,
	"name": "next-boilerplate",
	"version": "0.1.0",
	"scripts": {
		"build": "cross-env TZ=UTC next build && cross-env TZ=UTC tsc --project tsconfig.server.json",
		"ci": "npm run lint && npm run test && npm run build && npm run storybook:export",
		"dev": "nodemon",
		"lint": "eslint 'src/**/*.{js,ts,tsx,json}' '*.{js,ts,tsx,json}' '.storybook/**/*.{js,ts,tsx,json}'",
		"lint:fix": "npm run lint -- --fix",
		"ts": "tsc --project . && tsc --project tsconfig.server.json",
		"start": "cross-env TZ=UTC NODE_ENV=production node dist/server/index.js",
		"storybook": "start-storybook --ci -p 6006 -c .storybook",
		"storybook:export": "build-storybook -c .storybook -o public/storybook",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"generate:schema": "ts-node src/tasks/generate-schema.ts",
		"update": "npxm yarn-upgrade-minor && npxm yarn-check -u"
	},
	"dependencies": {
		"@sentry/browser": "^5.27.2",
		"@sentry/integrations": "^5.27.2",
		"@sentry/node": "^5.27.2",
		"@types/express": "^4.17.8",
		"@vercel/node": "^1.8.4",
		"@zeit/next-source-maps": "^0.0.4-canary.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"express": "^4.17.3",
		"graphql-zeus": "^2.8.5",
		"next": "^10.0.5",
		"nodemon": "^2.0.6",
		"react": "16.13.1",
		"react-dom": "16.13.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-flow": "^7.12.1",
		"@babel/preset-react": "^7.12.1",
		"@babel/preset-typescript": "^7.12.1",
		"@storybook/addon-controls": "^6.0.28",
		"@storybook/addon-docs": "^6.0.28",
		"@storybook/preset-typescript": "^3.0.0",
		"@storybook/react": "^6.0.28",
		"@types/enzyme": "^3.10.8",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^26.0.15",
		"@types/node": "^14.14.6",
		"@types/react": "^16.9.55",
		"@types/react-test-renderer": "^16.9.3",
		"@typescript-eslint/eslint-plugin": "^4.6.0",
		"@typescript-eslint/parser": "^4.6.0",
		"babel-jest": "^26.6.1",
		"babel-loader": "^8.1.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"babel-preset-react-app": "^9.1.2",
		"cross-env": "^7.0.2",
		"css-loader": "^4.3.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"eslint": "^7.12.1",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest": "^26.6.1",
		"prettier": "^2.2.1",
		"react-test-renderer": "^16.14.0",
		"sass": "^1.28.0",
		"sass-loader": "^10.0.4",
		"style-loader": "^1.3.0",
		"ts-jest": "^26.4.3",
		"ts-node": "^9.1.1",
		"typescript": "^4.0.5"
	}
}
