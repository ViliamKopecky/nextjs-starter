	},
	"devDependencies": {
		"@types/node": "^14.0.22",
		"@types/react": "^16.9.43",
		"@typescript-eslint/eslint-plugin": "^3.6.0",
		"@typescript-eslint/parser": "^3.6.0",
		"cross-env": "^7.0.2",
		"eslint": "^7.4.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"prettier": "^2.0.5",
		"ts-node": "^8.10.2",
		"typescript": "^3.9.6",
		"uvu": "^0.2.1"
