{
	"private": true,
	"name": "next-boilerplate",
	"version": "0.1.0",
	"scripts": {
		"build": "cross-env TZ=Europe/Prague next build",
		"dev": "cross-env TZ=Europe/Prague next dev",
		"lint": "eslint 'src/*/**/*.{js,ts,tsx,json}' '*.{js,ts,tsx,json}' --quiet",
